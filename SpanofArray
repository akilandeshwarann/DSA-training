import java.util.*;
public class Main {
    public static void main(String args[]){
        int arr[]={10,12,35,10 ,5,7,12};
        int max=Integer.MIN_VALUE;
        int min=Integer.MAX_VALUE;
        for(int i=0;i<arr.length;i++){
            if(max<arr[i]){
                max=arr[i];
            }
            if(min>arr[i]){
                min=arr[i];
            }
            
        }
        System.out.println(max-min);
    }
}
